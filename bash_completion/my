_complete_full()
{
    if [ $SHELL = $(which bash) ]; then
        local PATH=$HOME/_/Programs/bin
        _command_offset 1
    fi
    if [ $SHELL = $(which zsh) ]; then
        COMPREPLY=( $(compgen -c) )
    fi
} &&
complete -F _complete_full copy fkill parallel proc restart whatsin  dt dtb rdt rdtb rdtbo tb


_complete_by_options() {
    cur="${COMP_WORDS[COMP_CWORD]}"
    list=$(${COMP_WORDS[0]} help |sed -r 's/Options: (.*)/\1/')
    COMPREPLY=( $(compgen -W "$list" -- ${cur}) )
}
complete -F _complete_by_options light-mode vivaldi-windows
